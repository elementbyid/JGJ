<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="paneTime" prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" 
		xmlns:fx="http://javafx.com/fxml/1"
		fx:controller="useInfo.TimeAdd1"
		>
   <children>
      <SplitPane orientation="VERTICAL" prefHeight="150.0" prefWidth="400.0">
        <items>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="150.0" prefWidth="400.0">
               <children>
                  <HBox alignment="CENTER" layoutX="32.0" layoutY="49.0" spacing="15.0">
                     <children>
                        <ImageView fitHeight="50.0" fitWidth="50.0" layoutX="37.0" layoutY="50.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../../Desktop/image/anf.png" />
                           </image>
                        </ImageView>
                        <Label fx:id="lblAlert" layoutX="105.0" layoutY="67.0" text="">
                           <font>
                              <Font name="Arial Bold" size="12.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
               </children>
            </AnchorPane>
        </items>
      </SplitPane>
      <HBox alignment="CENTER" layoutX="189.0" layoutY="162.5" spacing="30.0">
         <children>
            <Button fx:id="btnOK"  layoutX="235.0" layoutY="163.0" 
            			mnemonicParsing="false" prefHeight="20.0" prefWidth="80.0" 
            			text="확인"
            			onAction="#handleBtnOkAction"
            			>
               <font>
                  <Font name="Arial" size="13.0" />
               </font>
            </Button>
            <Button fx:id="btnNo" layoutX="318.0" layoutY="163.0" mnemonicParsing="false" 
            			prefHeight="20.0" prefWidth="80.0" 
            			text="취소"
            			onAction="#handleBtnNoAction"
            			>
               <font>
                  <Font name="Arial" size="13.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
